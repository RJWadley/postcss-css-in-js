    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "json5": "^2.1.2",
    "mocha": "^7.1.1",
    "np": "^6.2.0",
    "nyc": "^15.0.0",
    "postcss": ">=7.0.27",
    "postcss-parser-tests": "^6.5.0",
    "postcss-safe-parser": "^4.0.2",
    "postcss-syntax": ">=0.36.2"
